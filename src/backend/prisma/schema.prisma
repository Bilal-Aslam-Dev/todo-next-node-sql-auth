// prisma/schema.prisma

datasource db {
  provider = "mysql"
  url      = "mysql://root@localhost:3306/todolist"
}

generator client {
  provider = "prisma-client-js"
  output   = "@prisma/client"
}

model todolist {
  id Int      @id @default(autoincrement())
  text String
  status Int
  date DateTime
}